#include <stdio.h>
#include "main.h"

/**
* _islower - prints 0 if letter is uppercase, else 1
* @c: represents letter
* Return: Always 0 (Success)
*
*/

int print_sign(int n)
{
if (n > 0)
  {
_putchar('+');
return (1);
  }
 else
   if(n == 0)
     {
     _putchar('0');
return (0);
     }
   else
     {
       _putchar('-');
 return (-1);
       }
	 
}
